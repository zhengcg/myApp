
.searchBox{
  height: 80rpx;
  background: #fff;
  padding: 10rpx 30rpx;
  box-sizing: border-box;
  border-bottom: 2rpx solid #F6F8FC
}
.searchDiv{
  height: 60rpx;
  position: relative;
  background: #F6F8FC;
}
.iconS{
  float: left;
  height: 60rpx;
  width: 76rpx;
  
  background: url('data:image/png;base64,') no-repeat center;
  background-size: 28rpx;
}
.searchDiv input{
  height: 60rpx;
  line-height: 60rpx;
  width: 524rpx;
  float: left;
  
}
.searchDiv .btnS{
  height: 60rpx;
  width: 90rpx;
  background: #fff;
  float: left;
  text-align: right;
  line-height: 60rpx;
  font-size: 32rpx;
color: #FF6262;
letter-spacing: 0.46rpx;

}
.orderNav{
  height: 84rpx;
  line-height: 84rpx;
  background: #fff;
  margin-bottom: 16rpx;

}
.orderNav>view{
  float: left;
  width: 33.333333%;
  text-align: center;
}
.orderNav text{
  display: inline-block;
  height: 80rpx;
  border-bottom:4rpx solid #FFF;
  padding: 0 20rpx;

}
.orderNav text.active{
  border-bottom:4rpx solid #FF6262;
  color:#FF6262
}
.orderList{
  background: #fff;
  margin-bottom: 16rpx;

}
.orderList .orderHead{
  height: 96rpx;
  line-height: 96rpx;
  border-bottom: 2rpx solid #F6F8FC;
  padding: 0 30rpx;
  color: #424242;
}
.orderList .orderHead view{
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
  width: 550rpx;
  display: inline-block;
}
.orderList .orderHead text{
  float: right;
  color: #FF9900;
}
.orderBody{
  padding:20rpx 30rpx 0 30rpx; 
}
.orderBody image{
  width: 690rpx;
  height: 170rpx;
  display: block;
  margin: 0 auto;
}
.orderFoot{
  height: 92rpx;
  line-height: 92rpx;
  padding: 0 30rpx;
}
.orderFoot text{
  margin-left: 30rpx;
}
.f1{
  margin-left: 0!important;
  padding-left:50rpx;
  background: url("data:image/png;base64,") no-repeat left center;
  background-size: 40rpx 40rpx;

}
.f2{
  background: url("data:image/png;base64,") no-repeat left center;
  background-size: 40rpx 40rpx;
  padding-left: 50rpx
}
.f3{
  background: url("data:image/png;base64,") no-repeat left center;
  background-size: 40rpx 40rpx;
  padding-left: 50rpx
}
.f4{
  float: right;
  color: #FF9900;
}

#shadeDiv{
  position: fixed;
  width: 100%;
  height: 100%;
  left: 0;
  top: 0;
  background: #000;
  opacity: 0.6;
}
.duiBox{
  position: absolute;
  background: #fff;
  width: 600rpx;
  height: 730rpx;
  left: 50%;
  top:124rpx;
  margin-left: -300rpx;
}
.duiBox .imgBox{
  width: 500rpx;
  padding: 50rpx;
  margin: 0 auto;
}
.duiBox .imgBox image{
  width: 500rpx;
  height: 500rpx;
  display: block;
  margin: 0 auto;
}
.duiBox .con{
  text-align: center;
  font-size: 32rpx;
  color: #FF6262;
  margin-top: 30rpx;
}


