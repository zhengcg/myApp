.searchBox{
  width: 690rpx;
  height: 56rpx;
  margin: 0 auto;
  background: #f6f8fc;
  border-radius: 12rpx;
  text-align: center;
  overflow: hidden;
}
.searchBox .btnBox{
  font-size: 24rpx;
  color: #52575A;
  height: 56rpx;
  line-height: 56rpx;
  display: inline-block;
  padding-left: 34rpx;
  background: url('data:image/png;base64,') no-repeat left center;
  background-size: 28rpx;
}

.head{
  height: 220rpx;
  overflow: hidden;
  position: relative;
}
.head image{
  width: 750rpx;
  height: 220rpx;
}
.selectBox{
  height: 80rpx;
  background: #F6F8FC;
  position: relative;
  margin-bottom: 16rpx;
}
.selectBox .borderBox{
  width: 3rpx;
  height: 50rpx;
  position: absolute;
  background: #C0C4C7;
  left: 50%;
  margin-left: -1.5rpx;
  top: 50%;
  margin-top: -25rpx;
}
.cardBox{
  width: 718rpx;
  height: 460rpx;
  background: #FFFFFF;
  border: 1rpx solid rgba(192,192,192,0.20);
  box-shadow: 0 3px 6px 0 rgba(0,0,0,0.05);
  border-radius: 16rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 0 32rpx;
  margin-bottom: 16rpx;
}
.cardTitle{
  height: 96rpx;
  line-height: 96rpx;
  white-space: nowrap;
  text-overflow: ellipsis;
  overflow: hidden;
  font-size: 36rpx;
  color: #111;
}
.cardBody{
  height: 260rpx;
  padding-bottom: 24rpx;
  border-bottom: 1rpx solid rgba(192,192,192,0.20);
  
  
}
.cardBody image{
    height:260rpx;
  }
  .cardFoot{
    line-height: 75rpx;
    font-size: 28rpx;
    color: #111;
  }
    .cardFoot view{display: inline-block;width: 40%;}
     .cardFoot view text{color: #FF6262}
     .picker{width: 350rpx;float: left;height: 80rpx;line-height: 80rpx;box-sizing: border-box;padding: 0 32rpx;font-size: 32rpx;color: #222;text-align: center;background: url('data:image/png;base64,') no-repeat right center; background-size: 24rpx 24rpx;}