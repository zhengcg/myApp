.searchBox{
  width: 690rpx;
  height: 56rpx;
  margin: 0 auto;
  background: #f6f8fc;
  border-radius: 12rpx;
  text-align: center;
  overflow: hidden;
}
.searchBox .btnBox{
  font-size: 24rpx;
  color: #52575A;
  padding-top: 14rpx;
}
.searchBox .btnBox icon{vertical-align: top;margin-right: 20rpx;display: inline;}


.head{
  height: 220rpx;
  overflow: hidden;
  position: relative;
}
.head image{
  width: 750rpx;
  height: 220rpx;
}
.selectBox{
  height: 80rpx;
  background: #F6F8FC;
  position: relative;
  margin-bottom: 16rpx;
}
.selectBox .borderBox{
  width: 3rpx;
  height: 50rpx;
  position: absolute;
  background: #C0C4C7;
  left: 50%;
  margin-left: -1.5rpx;
  top: 50%;
  margin-top: -25rpx;
}
.cardBox{
  width: 718rpx;
  height: 460rpx;
  background: #FFFFFF;
  border: 1rpx solid rgba(192,192,192,0.20);
  box-shadow: 0 3px 6px 0 rgba(0,0,0,0.05);
  border-radius: 16rpx;
  margin: 0 auto;
  box-sizing: border-box;
  padding: 0 32rpx;
}