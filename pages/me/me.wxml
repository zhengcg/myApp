<view class="container">
  <view class='head'>
    <view class='headImg'>
      <image src="{{avatarUrl}}"/>     
    </view>
    {{nickName}}
  </view>
  <view class='meTi'>
      您还可以免费做{{count}}次活动
  </view>
  <view class='meTab clearfix'>
    <view bindtap='gotoOrder'>
    <image src="../../images/icon/icon01.png" />  
    我制作的活动
    </view>
    <view bindtap='gotoActive'>
    <image src="../../images/icon/icon02.png"/>  
    参与的活动
    </view>
     <view  bindtap='gotoPay'>
     <image src="../../images/icon/icon03.png"/>  
     奖品兑换
     </view>
  </view>
  <view class='meList' bindtap='gotoStart'>
    <view class='con'><image src='../../images/icon/icon04.png'/><text>新手入门</text></view>   
  </view>
  <view class='meList' bindtap='gotoRaider'>
    <view class='con'><image src='../../images/icon/icon05.png'/><text>攻略</text></view>   
  </view>
  <view class='meList' bindtap='gotoPro'>
     <view class='con'><image src='../../images/icon/icon06.png'/><text>常见问题</text></view>   
  </view>
  <view class='meList' bindtap='gotoFeed'> 
    <view class='con' ><image src='../../images/icon/icon07.png'/><text>意见反馈</text></view>
  </view>
</view>
