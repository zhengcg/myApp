.head{
  height: 230rpx;
  background: #FF6262;
  padding: 0 64rpx 0 200rpx;
  position: relative;
  font-size: 36rpx;
  color: #FFFFFF;
  line-height: 230rpx;
}
.headImg{
  position: absolute;
  width: 108rpx;height: 108rpx;
  border-radius: 50%;
  overflow: hidden;
  left: 64rpx;
  top: 50%;
  margin-top: -54rpx;

}
.headImg image{width: 108rpx;height: 108rpx;display: block;}
.meTi{
  height: 120rpx;
  line-height: 120rpx;
  background: #fff;
  padding: 0 64rpx;
  color: #FF6262;
  margin-bottom: 16rpx;
}
.meTab{
  background: #fff;
  height: 144rpx;
  margin-bottom: 16rpx;
}
.meTab view{
  float: left;
  width: 33.33333%;
  text-align: center;
  color: #222;
}
.meTab view image{height: 43rpx;width: 36rpx;display: block;margin: 16rpx auto;}
.meList{
  background: #fff;
  height: 114rpx;
  border-bottom: 1rpx solid #DFDFDF;
  padding: 0 44rpx;
  line-height: 114rpx;
}
.meList:last-of-type{border: none;}
.meList>view{background: url('data:image/png;base64,') no-repeat right center;background-size: 10rpx 18rpx;}
.meList>view image{width: 50rpx;height: 50rpx;vertical-align: middle;margin-right: 16rpx;}
.meList>view text{vertical-align: middle;}