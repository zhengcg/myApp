.head{height: 350rpx;background: #52cac3;margin-bottom: 20rpx;padding: 30rpx;display: flex;justify-content: space-around;align-items:center;}
.headBox{width: 200rpx;}
.headBox image{width: 200rpx;height: 200rpx;border-radius: 50%;display:block;margin: 0 auto 30rpx auto }
.headBox view{text-align: center;color: #fff;font-size: 24rpx;line-height: 40rpx;}
.headBox view text{padding: 0 10rpx;}
.baseBox{color: #fff;line-height: 46rpx;}
.panel-body text{
    line-height: 1.5;
    color: #999;
    font-size: 24rpx;
    text-indent: 48rpx;
}
.progress{
    margin-bottom: 10rpx;

}
.proName{text-align: right;font-size: 24rpx;width: 120rpx;float: left;color: #666}
.proBar{margin-left: 150rpx;}