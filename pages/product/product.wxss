.nav{
  height: 80rpx;
  line-height: 80rpx;
  background: #fff;
  white-space: nowrap;
  position: fixed;
  top: 0;
}
.nav text{
  color: #222;
  margin: 0 35rpx;
  display: inline-block;
  height: 76rpx;
}
.nav text.cur{
  color: #23B6D0;
  border-bottom: 4rpx solid #23B6D0;
}
.labelBox{
  text-align: center;
  height: 60rpx;
  line-height: 60rpx;
  background: #777;
  color: #fff;
  position: fixed;
  top: 80rpx;
  width: 750rpx;
  
}
.proBox{
 padding:32rpx 0;
  background: #fff;
}
.proList{
  width: 352rpx;
  height: 652rpx;
  border: 1px solid rgba(0,0,0,0.05);
  border-radius: 16rpx;
  box-sizing: border-box;
  margin-left: 16rpx;
  margin-bottom: 16rpx;
  padding: 16rpx;
  float: left;
}
.proList image{width: 320rpx;height: 200rpx;}
.proList .tit{
  height: 36rpx;
  line-height: 36rpx;
  color: #111;
  margin-top: 16rpx;
}
.proList .tit view{
  float: right;
  background: #23B6D0;
  border-radius: 2px;
  color: #fff;
  font-size: 24rpx;
  padding: 4rpx 10rpx;
}
.proList .con{
  opacity: 0.6;
  font-size: 24rpx;
  color: #111111;
  letter-spacing: 0;
  line-height: 36rpx;
  margin-top: 16rpx;
  height: 144rpx;
  overflow: hidden;
  margin-bottom: 16rpx;
}
.proList .btn{
  font-size: 32rpx;margin-bottom: 16rpx;
}
